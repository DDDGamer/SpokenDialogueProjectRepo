<?xml version="1.0" encoding="utf-8" ?>
<grammar version="1.0" xml:lang="en-US" mode="voice" root="QuitCommands"
  xmlns="http://www.w3.org/2001/06/grammar" tag-format="semantics/1.0">
  <rule id="QuitCommands" scope="public">
    <item repeat="0-1">
      <one-of>
        <item> I want to </item>
      </one-of>
    </item>
    <item>
      <one-of>
        <item> quit </item>
        <item> end </item>
        <item> exit </item>
      </one-of>
    </item>
    <item repeat="0-1"> game </item>
  </rule>
</grammar>